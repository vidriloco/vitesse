<!DOCTYPE html>
<html>
	<head>
  		<title>Wikicleta : Rueda por tu ciudad</title>
  		<%= stylesheet_link_tag :defaults, 'lugares'  %>
  		<%= javascript_include_tag :defaults %>
		<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
  		<%= csrf_meta_tag %>
	</head>
	<body>
		<%= render 'compartidos/headers' %>
	
		<div class="unit on-2 columns same-height">
			<div class="lugares column fixed" style="width: 50%;">
				<%= yield %>
			</div>
			<div class="column fixed" style="width: 50%;">
				<div id="map_canvas" class="column fixed" style="width:100%; height:750px;"></div>
			</div>
		</div>
	</body>
</html>