<!DOCTYPE html>
<html>
	<head>
  		<title>Wikicleta : Rueda por tu ciudad</title>
  		<%= stylesheet_link_tag :defaults, :tokeninput, 'lugares'  %>
			
  		<%= javascript_include_tag :defaults, 'jquery.tokeninput', :extensions %>
			<% if Rails.env=="test"%>
				<%= javascript_include_tag :testing %>
			<% end %>
			<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
			<script src="http://code.google.com/apis/gears/gears_init.js" type="text/javascript"></script>
			
  		<%= csrf_meta_tag %>
	</head>
	<body>
		<%= render 'compartidos/headers' %>
	
		<div class="unit on-2 columns same-height">
			<div class="lugares column fixed" style="width: 50%;">
				<%= render 'acciones_menu' %>
				<div class="contenedor">
					<%= yield %>
				</div>
			</div>
			<div class="column fixed" style="width: 50%;">
				<div id="map_canvas" class="column fixed" style="width:100%; height:750px;"></div>
			</div>
		</div>
	</body>
</html>